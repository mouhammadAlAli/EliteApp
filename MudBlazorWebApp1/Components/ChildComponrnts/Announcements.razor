<MudCard Outlined="true">
    <MudCardHeader>
        <CardHeaderContent>
            <MudGrid Justify="Justify.FlexStart">
                <MudItem>
                    <MudIcon Color="Color.Primary" Icon="@Icons.Material.Outlined.Campaign" />
                </MudItem>
                <MudItem>
                    <MudText Color="Color.Primary">What's happening at Elite Medical Billing</MudText>
                </MudItem>
            </MudGrid>
            <MudDivider Style="width: 100%;" />
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
            <MudStack>
                <MudGrid>
                    <MudItem>
                        <MudIcon Color="Color.Primary" Icon="@Icons.Material.Outlined.School" />
                    </MudItem>
                    <MudItem>
                        <MudText Typo="Typo.body1">Training</MudText>
                        <MudText Typo="Typo.body2">3 Active Traininig, 4 past due or expired</MudText>
                    </MudItem>
                </MudGrid>
                <MudDivider />
                <MudGrid>
                    <MudItem>
                        <MudIcon Color="Color.Primary" Icon="@Icons.Material.Outlined.AttachMoney" />
                    </MudItem>
                    <MudItem>
                        <MudText Typo="Typo.body1">Benifits</MudText>
                        <MudText Typo="Typo.body2">enrolled in 4 benifit palns</MudText>
                    </MudItem>
                </MudGrid>
            </MudStack>
        <MudSpacer />
    </MudCardContent>
</MudCard>
