<MudCard Square="true" Outlined="true">
    <MudCardHeader>
        <CardHeaderContent>
            <MudGrid Justify="Justify.FlexStart">
                <MudItem>
                    <MudIcon Color="Color.Primary" Icon="@Icons.Material.TwoTone.Inventory" />
                </MudItem>
                <MudItem>
                    <MudText Color="Color.Primary">My Stuff</MudText>
                </MudItem>
            </MudGrid>
            <MudDivider Style="width: 100%;" />
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudStack>
            <MudGrid>
                <MudItem>
                    <MudIcon Color="Color.Primary" Icon="@Icons.Material.TwoTone.School" Size="Size.Large"/>
                </MudItem>
                <MudItem>
                    <MudText Typo="Typo.h6">Training</MudText>
                    <MudText Typo="Typo.body1">3 Active Traininig, 4 past due or expired</MudText>
                </MudItem>
            </MudGrid>
            <MudDivider/>
            <MudGrid>
                <MudItem>
                    <MudIcon Color="Color.Primary" Icon="@Icons.Material.TwoTone.AttachMoney" Size="Size.Large" />
                </MudItem>
                <MudItem>
                    <MudText Typo="Typo.h6">Benifits</MudText>
                    <MudText Typo="Typo.body1">enrolled in 4 benifit palns</MudText>
                </MudItem>
            </MudGrid>
        </MudStack>
        <MudSpacer />
    </MudCardContent>
</MudCard>
